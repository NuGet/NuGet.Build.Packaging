<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<!--
	============================================================
                             GetPackageContents
   Cross-targeting version of GetPackageContents. 
   NOTE: depends on Microsoft.Common.CrossTargeting.targets
   
   [IN]
   $(TargetFrameworks) - Semicolon delimited list of target frameworks.
   
   [OUT]
   @(InnerOutput) - The combined output items of the inner targets across
                    all builds.
  ============================================================
  -->
	<Target Name="GetPackageContents" DependsOnTargets="_SetBuildInnerTarget;DispatchToInnerBuilds" Returns="@(InnerOutput)" />

	<Target Name="_SetBuildInnerTarget">
		<PropertyGroup>
			<InnerTargets>GetPackageContents</InnerTargets>
		</PropertyGroup>
	</Target>

</Project>